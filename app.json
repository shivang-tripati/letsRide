
{
  "expo": {
    "name": "letsRide",
    "slug": "letsRide",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.letsRide",
      "config": {
        "googleMapsApiKey": "@env API_KEY"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.letsRide",
      "config" :{
        "googleMaps" : {
          "apikey":"@env API_KEY"
        },

        "permissons":["ACCESS_CORS_LOCATION", "ACCESS_FINE_LOCATION", "FOREGROUND_SERVICE"]
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
